{
   "action": {
      "default_popup": "menu.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://*.facebook.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Better FB Gaming. Emotes + Chat Extension with Settings",
   "icons": {
      "128": "icons/ico.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Better FB Gaming",
   "permissions": [ "storage", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}
